<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="4dp"
    app:cardElevation="4dp">

        <android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:textSize="18sp"
            android:fontFamily="sans-serif-medium"
            android:textStyle="normal"
            android:textColor="#1f2f42"
            android:lineSpacingExtra="6sp"
            android:text="@string/reception.reception_online"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:textSize="12sp"
            android:fontFamily="sans-serif"
            android:textStyle="normal"
            android:textColor="#8a1f2f42"
            android:text="@string/reception.chose"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView9"/>

        <TextView
            android:id="@+id/cosmetologist_text_view"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:textSize="16sp"
            android:fontFamily="sans-serif"
            android:textStyle="normal"
            android:textColor="#1f2f42"
            android:lineSpacingExtra="8sp"
            android:gravity="center_vertical"
            android:paddingLeft="40dp"
            android:text="@string/reception.cosmetologist"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView10"/>

        <View
            android:id="@+id/divider_1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f7f9"
            android:layout_marginLeft="48dp"
            app:layout_constraintTop_toBottomOf="@+id/cosmetologist_text_view"/>

        <TextView
            android:id="@+id/neurologist_text_view"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:textSize="16sp"
            android:fontFamily="sans-serif"
            android:textStyle="normal"
            android:textColor="#1f2f42"
            android:lineSpacingExtra="8sp"
            android:gravity="center_vertical"
            android:paddingLeft="40dp"
            android:text="@string/reception.neurologist"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divider_1"/>

        <View
            android:id="@+id/divider_2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f7f9"
            android:layout_marginLeft="48dp"
            app:layout_constraintTop_toBottomOf="@+id/neurologist_text_view"/>

        <TextView
            android:id="@+id/psychologist_text_view"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:textSize="16sp"
            android:fontFamily="sans-serif"
            android:textStyle="normal"
            android:textColor="#1f2f42"
            android:lineSpacingExtra="8sp"
            android:gravity="center_vertical"
            android:paddingLeft="40dp"
            android:text="@string/reception.psychologist"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.474"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divider_2"/>

        <View
            android:id="@+id/divider_3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f7f9"
            android:layout_marginLeft="48dp"
            app:layout_constraintTop_toBottomOf="@+id/psychologist_text_view"/>

        <TextView
            android:id="@+id/mammologist_text_view"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:textSize="16sp"
            android:fontFamily="sans-serif"
            android:textStyle="normal"
            android:textColor="#1f2f42"
            android:lineSpacingExtra="8sp"
            android:gravity="center_vertical"
            android:paddingLeft="40dp"
            android:text="@string/reception.mammologist"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divider_3"/>

        <View
            android:id="@+id/divider_4"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f7f9"
            android:layout_marginLeft="48dp"
            app:layout_constraintTop_toBottomOf="@+id/mammologist_text_view"/>

        <TextView
            android:id="@+id/ct_scan_text_view"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:textSize="16sp"
            android:fontFamily="sans-serif"
            android:textStyle="normal"
            android:textColor="#1f2f42"
            android:lineSpacingExtra="8sp"
            android:gravity="center_vertical"
            android:paddingLeft="40dp"
            android:text="@string/reception.ct_scan"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divider_4"/>

        <View
            android:id="@+id/divider_5"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f7f9"
            android:layout_marginLeft="48dp"
            app:layout_constraintTop_toBottomOf="@+id/ct_scan_text_view"/>

        <TextView
            android:id="@+id/mri_text_view"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:textSize="16sp"
            android:fontFamily="sans-serif"
            android:textStyle="normal"
            android:textColor="#1f2f42"
            android:lineSpacingExtra="8sp"
            android:gravity="center_vertical"
            android:paddingLeft="40dp"
            android:text="@string/reception.mri"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divider_5"/>

        <View
            android:id="@+id/divider_6"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#f4f7f9"
            android:layout_marginLeft="48dp"
            app:layout_constraintTop_toBottomOf="@+id/mri_text_view"/>

        <TextView
            android:id="@+id/x_ray_text_view"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:fontFamily="sans-serif"
            android:gravity="center_vertical"
            android:lineSpacingExtra="8sp"
            android:paddingLeft="40dp"
            android:text="@string/reception.x_ray"
            android:textColor="#1f2f42"
            android:textSize="16sp"
            android:textStyle="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divider_6"/>

    </android.support.constraint.ConstraintLayout>

</android.support.v7.widget.CardView>